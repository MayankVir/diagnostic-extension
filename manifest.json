{
      "name": "System Diagnostic Extension",
      "description": "System Diagnostic Extension",
      "version": "2.0",
      "manifest_version": 3,
      "icons": {
            "32": "./logo.png"
      },
      "background": {
            "service_worker": "background.js"
      },
      "content_scripts": [{
            "matches": ["http://*/*", "https://*/*"],
            "js": ["modules/cpu_data.js"]
      }],
      "permissions": [
            "tabs",
            "system.cpu",
            "system.display",
            "system.memory",
            "system.storage"
      ]
}